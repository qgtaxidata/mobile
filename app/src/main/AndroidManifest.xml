<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.taxidata">
    <!-- 允许程序打开网络套接字 -->
    <uses-permission android:name="android.permission.INTERNET" /> <!-- 允许程序设置内置sd卡的写权限 -->
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" /> <!-- 允许程序获取网络状态 -->
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" /> <!-- 允许程序访问WiFi网络信息 -->
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" /> <!-- 允许程序读写手机状态和身份 -->
    <uses-permission android:name="android.permission.READ_PHONE_STATE" /> <!-- 用于进行网络定位 -->
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" /> <!-- 用于访问GPS定位 -->
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" /> <!-- 用于获取wifi的获取权限，wifi信息会用来进行网络定位 -->
    <uses-permission android:name="android.permission.CHANGE_WIFI_STATE" /> <!-- 用于读取手机当前的状态 -->
    <uses-permission android:name="android.permission.READ_PHONE_STATE" /> <!-- 用于申请调用A-GPS模块 -->
    <uses-permission android:name="android.permission.ACCESS_LOCATION_EXTRA_COMMANDS" />

    <application
        android:name=".application.TaxiApp"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:networkSecurityConfig="@xml/network_security_config"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ui.roadquality.RoadQualityActivity"></activity>
        <activity android:name=".ui.areaincome.AreaIncomeActivity" />
        <activity android:name=".ui.passengerpath.view.OriginActivity" />
        <activity android:name=".ui.passengerpath.view.OriginEndActivity" />
        <activity android:name=".ui.passengerpath.view.EndActivity" />
        <activity android:name=".ui.setup.SetUpActivity" />
        <activity android:name=".ui.areaanalyze.AreaAnalyzeActivity" />
        <activity android:name=".ui.hotspot.view.HotSpotPathActivity" />
        <activity android:name=".ui.IncomeRanking.IncomeRankingActivity" />
        <activity android:name=".test.MainActivity" />
        <activity android:name=".ui.hotspot.view.HotSpotResearchActivity" />
        <activity android:name=".ui.hotspot.view.OriginHotSpotActivity" />
        <activity
            android:name=".HomePageActivity"
            android:launchMode="standard">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <action android:name="android.intent.action.VIEW" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ui.TaxiPath.TaxiPathActivity" />
        <activity android:name=".ChartText.PieChartActivity" />
        <activity android:name=".ChartText.BarChartActivity" />
        <activity android:name=".ChartText.LineChartActivity" />

        <meta-data
            android:name="com.amap.api.v2.apikey"
            android:value="2002b6e6cb4527690f56dab4374fc825" />
    </application>

</manifest>